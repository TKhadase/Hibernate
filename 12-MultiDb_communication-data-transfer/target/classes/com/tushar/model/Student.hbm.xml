<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.tushar.model.Student"  table="Student"  dynamic-insert="true" dynamic-update="true">
		<id name="SID" column="SID" unsaved-value="155" >
			<generator class="increment"></generator>
		</id>
		<property name="sname" column="SNAME" />
		<property name="marks" column="MARKS" />
		<property name="result" column="RESULT" />
		<property name="percent" column="PERCENT" />
	</class>
</hibernate-mapping>